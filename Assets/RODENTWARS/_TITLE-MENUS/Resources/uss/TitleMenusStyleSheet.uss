.titles-header {
    flex-grow: 1;
    flex-direction: row;
    -unity-font-style: bold;
    -unity-text-outline-color: rgb(255, 255, 0);
    -unity-text-outline-width: 1px;
    -unity-font-definition: url('project://database/Assets/RODENTWARS/_TITLE-MENUS/Resources/fonts/FeastOfFleashBI.TTF?fileID=12800000&guid=17407ba1fd0c7e549a0a9e3dd798fca8&type=3#FeastOfFleashBI');
