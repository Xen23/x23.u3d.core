    -unity-slice-left: 0;
    -unity-slice-top: 0;
    -unity-slice-right: 0;
    -unity-slice-bottom: 0;
    width: auto;
    height: auto;
    margin-right: 0;
    margin-left: 0;
    transition-property: -unity-background-image-tint-color;
    transition-duration: 2s;
    transition-timing-function: ease-in-out;
    margin-top: 0;
    margin-bottom: 0;
    align-items: stretch;
    font-size: 80px;
    position: absolute;
    right: 0;
    left: 0;
}

