    -unity-slice-left: 50;
    -unity-slice-top: 50;
    -unity-slice-right: 50;
    -unity-slice-bottom: 50;
    -unity-slice-scale: initial;
    padding-top: 3%;
    padding-bottom: 3%;
    display: flex;
}

#PANEL_MAIN:inactive {
    transition-property: translate;
    transition-duration: 1s;
}

#PANEL_MAIN:hover {
    -unity-background-image-tint-color: rgba(255, 255, 255, 0.79);
}

#PANEL_MAIN:active {
    transition-property: translate;
    transition-duration: 1s;
}

#PANEL_MAIN:focus {
    transition-property: translate;
    transition-duration: 1s;
}

#Toggle .titles-button_bg {
    display: none;
    flex-shrink: 0;
    flex-grow: 1;
    visibility: visible;
    overflow: visible;
}

#Toggle > Label {
    flex-grow: 0;
    flex-shrink: 0;
    flex-direction: column;
    align-items: stretch;
    font-size: 69px;
    -unity-text-outline-width: 2.5px;
    -unity-font-definition: resource('fonts/FeastOfFleashBI');
}

#Toggle:checked .titles-button_bg {
    display: flex;
    flex-shrink: 0;
    flex-grow: 1;
}
