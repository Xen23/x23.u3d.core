.titles-panel {
    flex-grow: 1;
    flex-shrink: 0;
    background-image: url('project://database/Assets/Standard%20Resources/Fantasy%20Wooden%20GUI%20%20Free/normal_ui_set%20A/UI%20board%20Large%20Set.png?fileID=2800000&guid=90a2dde0942123c4ebb9c4af580c5818&type=3#UI board Large Set');
